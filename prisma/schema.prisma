
// Gerando o cliente prisma
generator client {
  provider = "prisma-client-js"
}

// indicando qual o Banco de Dados (MongoDB) e qual a vari√°vel que vai armazenar o caminho para ele
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// Modelo a ser seguido para armazenar um novo registro no Banco de Dados
model Gestante {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  nome    String
  data   String   
  endereco String
  telefone String
  equipe String
  parceiro String?
  dum   String  
  gestacoes    Int
}